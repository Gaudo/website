{
    "rules": {
        "semicolon": [true, "always"],
        "curly": true,
        "forin": true,
        "label-position": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-invalid-this": [true, "check-function-in-method"],
        "no-switch-case-fall-through": true,
        "no-unreachable": true,
        "no-unused-expression": true,
        "no-unused-variable": [true, "react"],
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "radix": true,
        "switch-default": true,
        "triple-equals": true,
        "use-isnan": true,
        "use-strict": [true, "check-module", "check-function"],
        "indent": [true, "spaces"],
        "no-require-imports": true,
        "no-trailing-whitespace": true,
        "object-literal-sort-keys": true,
        "no-default-export": true,
        "max-line-length": [true, 80],
        "class-name": true,
        "comment-format": [true, "check-space", "check-uppercase"],
        "interface-name": [true, "never-prefix"],
        "jsdoc-format": true,
        "new-parens": true,
        "no-angle-bracket-type-assertion": true,
        "no-consecutive-blank-lines": true,
        "quotemark": [true, "single", "avoid-escape"],
        "variable-name": [true, "ban-keywords", "check-format"],
        "whitespace": [true, "check-branch", "check-operator", "check-typecast", "check-decl", "check-module", "check-type", "check-separator"],
        "member-access": [true, "check-accessor"],
        "no-internal-module": true,
        "no-reference": true,
        "no-var-requires": true,
        "typedef": [true, "call-signature", "parameter", "member-variable-declaration", "arrow-parameter", "property-declaration", "variable-declaration"]
    }
}
